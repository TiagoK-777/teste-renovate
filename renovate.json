{
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^config\\.yaml$/"
      ],
      "matchStrings": [
        "redis:\\n(?:\\s*[^\\n]+\\n)*\\s*version:\\s*\"?(?<currentValue>.*?)\"?"
      ],
      "datasourceTemplate": "docker",
      "packageNameTemplate": "redis"
    }
  ]
}