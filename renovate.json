{
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^config\\.yaml$/"
      ],
      "matchStrings": [
        "^redis:\\n\\s+version:\\s*\"?(?<currentValue>.*?)\"?\\s*"
      ],
      "datasourceTemplate": "docker",
      "packageNameTemplate": "redis"
    }
  ]
}