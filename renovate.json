{
  "extends": [
    "config:recommended"
  ],
  "repositories": [
    "TiagoK-777/teste-renovate"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^config\\.yaml$/"
      ],
      "matchStrings": [
        "version:\\s+\"(?<currentValue>v?)(?<version>.*?)\""
      ],
      "depNameTemplate": "wppconnect-team/wppconnect-server",
      "datasourceTemplate": "github-releases",
      "packageNameTemplate": "wppconnect-team/wppconnect-server",
      "currentValueTemplate": "{{version}}"
    }
  ]
}
