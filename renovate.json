{
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^config\\.yaml$/"
      ],
      "matchStrings": [
        "image:\\s+redis:(?<currentValue>.*?)"
      ],
      "datasourceTemplate": "docker",
      "packageNameTemplate": "redis"
    }
  ]
}
