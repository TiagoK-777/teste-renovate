{
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^config\\.yaml$/"
      ],
      "matchStrings": [
        "version:\\s*\"?(?<currentValue>[\\d.]+)-.*\"?\\s*"
      ],
      "datasourceTemplate": "github-releases",
      "packageNameTemplate": "redis/redis"
    }
  ]
}