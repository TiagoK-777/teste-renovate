{
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^config\\.yaml$/"
      ],
      "matchStrings": [
        "version:\\s*\"(?<currentValue>[\\w.]+)\"\\s*"
      ],
      "datasourceTemplate": "docker",
      "packageNameTemplate": "selenium/standalone-chrome"
    }
  ]
}